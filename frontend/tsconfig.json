{
  "compilerOptions": {
    "lib": [
      "dom",
    ],
    "noEmit": true,
    "paths": {
      "@common/*": [
        "../../common/*"
      ]
    },
    "baseUrl": "./src",
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "strict": false,
    "outDir": "../dist/frontend",
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}